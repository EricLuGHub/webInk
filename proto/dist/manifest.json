{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0",
  "description": "Printsss Hello World in console",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Click me",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "permissions": ["storage","activeTab","scripting"]
}
