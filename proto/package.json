{
  "manifest_version": 3,
  "name": "Web Ink",
  "version": "1.0.0",
  "description": "LOLZ",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "@types/node": "^24.1.0"
  },
  "dependencies": {
    "vite": "^7.0.6"
  }
}
